<script type="text/worker">
    const buttonlist = ["character","pc", "party"];
    buttonlist.forEach(button => {
        on(`clicked:${button}`, function() {
            setAttrs({
                sheetTab: button
            });
        });
    });
	on("change:party1hp sheet:opened", function() {
		getAttrs(["party1hp"], function(values) {
			let hp = parseInt(values.party1hp)||0;
			setAttrs({
				"party1maxhp": hp
			});
		});
	});
	on("change:party2hp sheet:opened", function() {
		getAttrs(["party2hp"], function(values) {
			let hp = parseInt(values.party2hp)||0;
			setAttrs({
				"party2maxhp": hp
			});
		});
	});
	on("change:party3hp sheet:opened", function() {
		getAttrs(["party3hp"], function(values) {
			let hp = parseInt(values.party3hp)||0;
			setAttrs({
				"party3maxhp": hp
			});
		});
	});
	on("change:party4hp sheet:opened", function() {
		getAttrs(["party4hp"], function(values) {
			let hp = parseInt(values.party4hp)||0;
			setAttrs({
				"party4maxhp": hp
			});
		});
	});
	on("change:party5hp sheet:opened", function() {
		getAttrs(["party5hp"], function(values) {
			let hp = parseInt(values.party5hp)||0;
			setAttrs({
				"party5maxhp": hp
			});
		});
	});
	on("change:party6hp sheet:opened", function() {
		getAttrs(["party6hp"], function(values) {
			let hp = parseInt(values.party6hp)||0;
			setAttrs({
				"party6maxhp": hp
			});
		});
	});
</script>
